{
    "env": {
        "browser": true,
        "commonjs": true,
        "es6": true,
        "node": true
    },
    "extends": [
        "standard"
    ],
    "globals": {
        "Atomics": "readonly",
        "SharedArrayBuffer": "readonly"
    },
    "parserOptions": {
        "ecmaVersion": 11
    },
    "rules": {
        "no-unused-vars": 0,
        "no-trailing-spaces": 0,
        "handle-callback-err": 0,
        "space-before-function-paren": 0,
        "eol-last": 0,
        "curly": 0,
        "quotes": [0, "double"],
        "no-multiple-empty-lines": 0,
        "spaced-comment": 0,
        "func-call-spacing": 0,
        "no-unexpected-multiline": 0,
        "padded-blocks": 0,
        "object-curly-spacing": 0,
        "no-undef": 0,
        "indent": 0,
        "no-useless-return": 0,
        "prefer-const": 0
    }
}
